{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Chrome Extension",
  "description": "A base setup for chrome extension with react and webpack",
  "action": {
    "default_title": "Pondir",
    "default_popup": "popup.html"
  },
  "icons": {
