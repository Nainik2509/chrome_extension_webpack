    "48": "/images/icons/icon-48.png",
    "72": "/images/icons/icon-72.png",
    "96": "/images/icons/icon-96.png",
    "144": "/images/icons/icon-144.png",
    "192": "/images/icons/icon-192.png"
  },
  "default_locale": "en"
}
